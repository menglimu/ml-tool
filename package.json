{
  "name": "ml-tools-dev",
  "version": "1.0.1",
  "description": "工具集",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "route": "node --experimental-specifier-resolution=node --loader ts-node/esm src/route2excel/index.ts",
    "api": "node --experimental-specifier-resolution=node --loader ts-node/esm src/api/index.ts",
    "ml-version": "node --experimental-specifier-resolution=node --loader ts-node/esm src/version/index.ts",
    "build": "rollup -c",
    "patch": "npm run build && npm version patch && npm publish",
    "test": "jest"
  },
  "bin": {
    "ml-version": "./bin/ml-version.js",
    "ml-api": "./bin/ml-api.js"
  },
  "author": "wenlin",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.25",
    "excel-export": "^0.5.1",
    "jest": "^27.5.1",
    "rollup": "^2.67.3",
    "rollup-plugin-clear": "^2.0.7",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  }
}
